<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DeepKnowledgePlatform</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/DeepKnowledgePlatform.styles.css" asp-append-version="true" />
</head>
<body>
    @await Component.InvokeAsync("AdminTopNav");
    <div class="layoutSidenav">
        <div class="layoutSidenav_nav">
            @await Component.InvokeAsync("AdminSideNav");
        </div>
        <div class="layoutSidenav_content">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
            @await Component.InvokeAsync("AdminFooter");
        </div>
    </div>
    

    

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>